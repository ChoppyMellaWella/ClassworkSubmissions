
import java.util.ArrayList;

public class ArrayListIntro 
{

	public static void main(String[] args) 
	{

		ArrayList<String> yea = new ArrayList<String>();
		
//		Add the following String literals to studentNames: 
//		"Alice", "Charlie", "Dave", "Elizabeth", "Fieri", "Gwen". 
//		The ArrayList should hold them in this order. Print the 
//		ArrayList to make sure it is organized correctly.
		
		yea.add("Alice");
		yea.add("Charlie");
		yea.add("Dave");
		yea.add("Elizabeth");
		yea.add("Fieri");
		yea.add("Gwen");
		
		yea.add(1, "Brittany");
		
		System.out.println(yea);
		
		yea.remove(1);
		
		System.out.println(yea.get(2));
		
		yea.set(3, "Evan");
		
		
	}

}
